---
import BaseLayout from "../../layouts/base.astro";
import Content from "../../components/Content.astro";

import { Image } from 'astro:assets';

const { title, images = [] } = Astro.props;
---

<BaseLayout title={title}>
	<Content title={title}>
		<slot />

		{images.map((src) => (
			<Image src={src} alt="" />
		))}
	</Content>
</BaseLayout>