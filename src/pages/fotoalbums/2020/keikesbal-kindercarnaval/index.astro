---
import FotoAlbum from '../../_fotoalbum.astro';

export const title = "Keikesbal kindercarnaval"

// Glob all images in a specific directory
const imageImports = import.meta.glob('../../../../../public/images/fotoalbums/2020/keikesbal-kindercarnaval/*.{jpeg,jpg,png,gif}', { eager: true });

// Convert the glob object into an array of image imports
const images = Object.values(imageImports).map((img) => img.default);
---

<FotoAlbum title={title} images={images}>
	In samenwerking met OVW8
</FotoAlbum>

