  "keywords": [],
  "author": "Sjors Spoorendonk",
  "license": "ISC",
  "homepage": "https://github.com/sjorsjes/buurthuiskeijenburg#readme",
  "bugs": {
    "url": "https://github.com/sjorsjes/buurthuiskeijenburg/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sjorsjes/buurthuiskeijenburg.git"
  },
  "scripts": {
    "debug": "set DEBUG=Eleventy* & npm run dev",
    "dev": "npm-run-all --parallel dev:*",
    "dev:css": "postcss ./source/_statics/css/main.css --map --output ./source/_statics/css/main.min.css --watch true",
    "dev:eleventy": "eleventy --input=source --output=dist --serve --port=3000",
    "build:css": "postcss ./source/_statics/css/main.css --map --output ./source/_statics/css/main.min.css",
    "build:eleventy": "eleventy --input=source --output=dist --pathprefix='/buurthuiskeijenburg/'"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@11ty/eleventy-navigation": "^0.1.3",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.26",
    "postcss-clean": "^1.1.0",
    "postcss-cli": "^7.1.0",
    "postcss-custom-media": "^7.0.8",
    "postcss-custom-properties": "^9.0.2",
    "postcss-import": "^12.0.1",
    "postcss-media-variables": "^2.0.1",
    "postcss-nested": "^4.2.1",
    "stylelint": "^13.0.0",
    "stylelint-config-vi": "^1.5.0"
  },
  "dependencies": {}
}
